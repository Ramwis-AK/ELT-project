SELECT COUNT() FROM ANALYTICS.FACT_AD_DAILY WHERE CAMPAIGN_ID NOT IN (SELECT CAMPAIGN_ID FROM ANALYTICS.DIM_CAMPAIGN);
SELECT COUNT() FROM ANALYTICS.FACT_AD_DAILY WHERE AD_ID NOT IN (SELECT AD_ID FROM ANALYTICS.DIM_AD);
SELECT COUNT(*) FROM ANALYTICS.FACT_AD_DAILY WHERE ACCOUNT_ID NOT IN (SELECT ACCOUNT_ID FROM ANALYTICS.DIM_ACCOUNT);

SELECT 
    COUNT(DISTINCT ad_date) as days_with_data,
    COUNT(DISTINCT CAMPAIGN_ID) as campaigns,
    COUNT(DISTINCT AD_ID) as ads,
    COUNT(DISTINCT ACCOUNT_ID) as accounts
FROM ANALYTICS.FACT_AD_DAILY;
